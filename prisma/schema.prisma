datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider             = "prisma-client-py"
  recursive_type_depth = 5,
  interface = "sync"
}

model Assistants {
  id        Int     @id @default(autoincrement())
  name     String
  description   String?
  parameters  Json
}
